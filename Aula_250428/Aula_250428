class Carro:
    def __init__(self, marca, modelo, ano):
        self.marca = marca
        self.modelo = modelo
        self.ano = ano

    def descrever(self):
        print(f"{self.marca} {self.modelo}, Ano: {self.ano}")

# Criando objetos da classe Carro
carro1 = Carro("Toyota", "Corolla", 2020)
carro2 = Carro("Honda", "Civic", 2018)

# Chamando m√©todos
carro1.descrever()
carro2.descrever()